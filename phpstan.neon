parameters:
    level: 5
    paths:
        - app
    excludePaths:
        - tests/Unit/ExampleTest.php
        - tests/Feature/Auth/*
        - vendor/*
    ignoreErrors:
        - '#PHPDoc tag @mixin contains unknown class Eloquent#'
        - '#Call to an undefined static method [a-zA-Z0-9\\_]+::(find|create|where|get|select)\(\)#'
        - '#Access to an undefined property [a-zA-Z0-9\\_]+::\$[a-zA-Z0-9\\_]+#'
        - '#Call to an undefined method [a-zA-Z0-9\\_]+::[a-zA-Z0-9\\_]+\(\)#'
